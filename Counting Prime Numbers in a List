def count_primes(num):
    
    # Check for 0 or 1
    if num < 2:
        return 0
    ###############
    # 2 or greater
    ##############
    
    #Store out prime numbers
    primes = [2]
    #Counter going up to the input
    x = 3
    
    while x <= num:
        #Checks if x is prime
        for y in range(3,x,2):
            if x%y == 0:
                x += 2
                break
        else:
            primes.append(x)
            x += 2
    print(primes)
    return len(primes)
